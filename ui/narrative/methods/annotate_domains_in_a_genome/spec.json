{
    "ver": "1.0.3",
    "authors": [
	"jmc", "psnovichkov", "rsutormin"
    ],
    "contact": "help@kbase.us",
    "visible": true,
    "categories": ["active", "annotation"],
    "widgets": {
	"input": null,
	"output": "kbaseDomainAnnotation"
    },
    "parameters": [ 
	{
	    "id" : "genome_ref",
	    "optional" : false,
	    "advanced" : false,
	    "allow_multiple" : false,
	    "default_values" : [ "" ],
	    "field_type" : "text",
	    "text_options" : {
		"valid_ws_types" : [ "KBaseGenomes.Genome" ]
	    }
	},
	{
	    "id" : "dms_ref",
	    "optional" : false,
	    "advanced" : false,
	    "allow_multiple" : false,
	    "default_values" : [ "KBasePublicGeneDomains/All" ],
	    "field_type" : "dropdown",
	    "dropdown_options":{
		"options": [
		    {
			"value": "KBasePublicGeneDomains/All-v2",
			"display": "All domain libraries"
		    },
		    {
			"value": "KBasePublicGeneDomains/COGs-only-v2",
			"display": "COGs 1.0 (from CDD 3.16) domains only"
		    },
		    {
			"value": "KBasePublicGeneDomains/NCBI-CDD-only-v2",
			"display": "NCBI-curated CDD 3.16 domains only"
		    },
		    {
			"value": "KBasePublicGeneDomains/SMART-only-v2",
			"display": "SMART 6.0 (from CDD 3.16) domains only"
		    },
		    {
			"value": "KBasePublicGeneDomains/Pfam-only-v2",
			"display": "Pfam 31 domains only"
		    },
		    {
			"value": "KBasePublicGeneDomains/TIGRFAMs-only-v2",
			"display": "TIGRFAM 15 domains only"
		    }
		]
	    }
	},
	{
	    "id" : "output_result_id",
	    "optional" : false,
	    "advanced" : false,
	    "allow_multiple" : false,
	    "default_values" : [ "" ],
	    "field_type" : "text",
	    "text_options" : {
		"valid_ws_types" : [ "KBaseGeneFamilies.DomainAnnotation" ],
		"is_output_name":true
	    }
	}
    ],    
    "behavior": {
	"service-mapping": {
	    "url": "",
	    "name": "DomainAnnotation",
	    "method": "search_domains",
	    "input_mapping": [
		{
		    "input_parameter": "genome_ref",
		    "target_property": "genome_ref",
		    "target_type_transform": "resolved-ref"
		},
		{
		    "input_parameter": "dms_ref",
		    "target_property": "dms_ref"
		},
		{
		    "narrative_system_variable": "workspace",
		    "target_property": "ws"
		},
		{
		    "input_parameter": "output_result_id",
		    "target_property": "output_result_id",
		    "generated_value": {
			"prefix": "domains_"
		    }
		}
	    ],
	    "output_mapping": [
                {
                    "service_method_output_path": [0, "output_result_id"],
                    "target_property": "output_result_id"
                },
                {
                    "service_method_output_path": [0, "report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0, "report_ref"],
                    "target_property": "report_ref"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspaceID"
                },
                {
                    "input_parameter": "output_result_id",
                    "target_property": "domainAnnotationID"
                }
	    ]
	}
    },
    "job_id_output_field": "docker"
}
